+  })
+
+  it(`allows creating nodes`, () => {
+    const action = actions.createNode(
+      {
+        id: `hi`,
+        children: [],
+        parent: `test`,
+        internal: {
+          contentDigest: `hasdfljds`,
+          type: `Test`,
+        },
+        pickle: true,
+      },
+      {
+        name: `tests`,
+      }
