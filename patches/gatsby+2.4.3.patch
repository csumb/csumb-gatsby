+
+const getNodeById = (nodeStore, id) => {
+  // This is for cases when the `id` has already been resolved
+  // to a full Node for the input filter, and is also in the selection
+  // set. E.g. `{ foo(parent: { id: { eq: 1 } } ) { parent { id } } }`.
