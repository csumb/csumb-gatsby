+                anObjectArray {
+                  aString
+                  aNumber
+                  aBoolean
+                }
+              }
+            }
+          }
+        }
+      `
+    )
+    expect(result.errors).not.toBeDefined()
+
+    expect(result).toMatchSnapshot()
+  })
