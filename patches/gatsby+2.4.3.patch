+    store.dispatch({ type: `DELETE_CACHE` })
+    nodes.forEach(node =>
+      store.dispatch({ type: `CREATE_NODE`, payload: { ...node } })
+    )
+  })
+
+  it(`removes empty input filter fields`, async () => {
+    // This can happen when a type has only one GraphQLUnion type field,
