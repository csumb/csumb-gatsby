+          fieldTypeComposer = ObjectTypeComposer.createTemp(
+            createTypeName(selector),
+            schemaComposer
+          )
+        }
+
+        return {
+          type: addInferredFieldsImpl({
+            schemaComposer,
+            typeComposer: fieldTypeComposer,
+            nodeStore,
+            exampleObject: value,
+            typeMapping,
+            prefix: selector,
+            addNewFields,
+            addDefaultResolvers,
+          }),
+        }
+      }
