+  throw new Error(`Can't determine type for "${value}" in \`${selector}\`.`)
+}
+
+const createTypeName = selector => {
+  const keys = selector.split(`.`)
+  const suffix = keys
+    .slice(1)
+    .map(_.upperFirst)
+    .join(``)
+  return `${keys[0]}${suffix}`
