+}
+
+const getSimpleFieldConfig = ({
+  schemaComposer,
+  typeComposer,
+  nodeStore,
+  key,
+  value,
+  selector,
+  typeMapping,
+  addNewFields,
+  addDefaultResolvers,
+}) => {
+  switch (typeof value) {
+    case `boolean`:
+      return { type: `Boolean` }
+    case `number`:
+      return { type: is32BitInteger(value) ? `Int` : `Float` }
+    case `string`:
+      if (isDate(value)) {
+        return dateResolver
+      }
