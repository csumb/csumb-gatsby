+      description: `Don't add default resolvers to defined fields.`,
+      deprecationReason: `noDefaultResolvers is deprecated, annotate individual fields.`,
+    },
+  },
+})
+
+module.exports = {
+  InferDirective,
+  DontInferDirective,
+}
