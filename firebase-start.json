{
  "hosting": {
    "public": "public",
    "trailingSlash": false,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/cloud-functions/okta/secondary-email",
        "function": "oktaSecondaryEmail"
      },
      {
        "source": "/cloud-functions/okta/session-end",
        "function": "oktaSessionEnd"
      },
      {
        "source": "/cloud-functions/laundry",
        "function": "laundry"
      },
      {
        "source": "/cloud-functions/laundry/hall/:hall",
        "function": "laundry"
      }
    ]
  }
}
