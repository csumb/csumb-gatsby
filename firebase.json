{
  "hosting": {
    "public": "public",
    "trailingSlash": false,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "_last-build.json",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ],
    "redirects": [
      { "source": "/cms-help", "destination": "/oasis", "type": 301 },
      { "source": "/itcd", "destination": "/scd", "type": 301 },
      { "source": "/innovation", "destination": "/iied", "type": 301 },
      {
        "source": "/financial-aid",
        "destination": "/financialaid",
        "type": 301
      },
      { "source": "/marketing", "destination": "/affairs", "type": 301 },
      {
        "source": "/academic-affairs",
        "destination": "/academicaffairs",
        "type": 301
      },
      {
        "source": "/cms-error",
        "destination": "/oasis/whoops-looks-something-went-wrong",
        "type": 301
      },
      {
        "source": "/id",
        "destination": "/it/accounts-and-passwords",
        "type": 301
      },
      {
        "source": "/ephs",
        "destination": "/police/health-and-safety",
        "type": 301
      },
      {
        "source": "/housingapp",
        "destination": "https://csumb.starrezhousing.com/StarRezPortal/Site/_General/ShibbolethInCommon/Shibboleth.aspx",
        "type": 301
      },
      { "source": "/map", "destination": "/about/map-directions", "type": 301 }
    ]
  }
}
